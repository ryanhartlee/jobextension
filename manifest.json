{
  "manifest_version": 2,
  "name": "Job Ninja",
  "version": "0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.4.0.min.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "images/get_started16.png"
  }
}
